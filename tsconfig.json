{
  "compilerOptions": {
    "outDir": "dist/",
    "sourceMap": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "target": "ES6",
    "declaration": true,
    "skipLibCheck": true,
    "removeComments": false,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "pretty": true,
    "esModuleInterop": true
  },
  "include": ["src"]
}
